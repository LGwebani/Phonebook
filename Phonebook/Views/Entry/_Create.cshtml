<div class="modal" tabindex="-1" role="dialog" id="createModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Add Phonebook</h5>
                <button type="button" class="close" data-bind="click: $root.closeCreateEditDelete;" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <partial name="_CreateEdit" />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bind="click: $root.createItem">Yes</button>
                <button type="button" class="btn btn-secondary" data-bind="click: $root.closeCreateEditDelete;" data-dismiss="modal">No</button>
            </div>
        </div>
    </div>
</div>